                "-IF:/SDL/SDL2-2.28.2/include",
                "-IF:/SDL/SDL2_image-2.6.3/include",
                "-IF:/SDL/SDL2_mixer-2.6.3/include",
                "-IF:/SDL/SDL2_ttf-2.20.2/include",
                "-IF:/SDL/SDL2_net-2.2.0/include",
                "-LF:/SDL/SDL2-2.28.2/lib/x64",
                "-LF:/SDL/SDL2_image-2.6.3/lib/x64",
